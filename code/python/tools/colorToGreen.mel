//Curve/Wireframe Color - Blue
string $selClr[] = `ls -sl`;
//For all objects in selection, run the folllowing commands..
for ($all in $selClr){
    //Enable color override...
    setAttr ($all + ".overrideEnabled") 1;
    //Recolor curve/wireframe Blue...
    setAttr ($all + ".overrideColor") 14;

}